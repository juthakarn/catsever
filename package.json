{
  "name": "catsever",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server/server.js",
    "model:init": "node_modules/.bin/sequelize init",
    "model:migrate": "node_modules/.bin/sequelize db:migrate",
    "model:seedall": "node_modules/.bin/sequelize db:seed:all",
    "createmodel": "npm-run-all --parallel createmodel:*",
    "createmodel:user": "node_modules/.bin/sequelize model:generate --name users --attributes password:string,firstname:string,lastname:string,email:string",
    "createmodel:findcats": "node_modules/.bin/sequelize model:generate --name findcats --attributes latitude:double,longitude:double,address:string,contact:string,message:string,imagepath:string",
    "createmodel:news": "node_modules/.bin/sequelize model:generate --name news --attributes title:string,subtitle:string,content:string,image:string"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.2",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "multer": "^1.4.1",
    "mysql2": "^1.6.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.41.1",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}